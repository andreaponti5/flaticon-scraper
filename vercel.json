{
    "builds": [
        {
            "src": "app/app.py",
            "use": "@vercel/python"
        },
        {
            "src": "app/scraper.py",
            "use": "@vercel/python"
        },
        {
            "src": "app/__init__.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/app.py"
        }
    ]
}